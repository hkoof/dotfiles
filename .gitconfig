[alias]
    lol = log --oneline --graph --decorate=short --relative-date
    flog = log --graph --pretty='format:%C(yellow)%h%Creset%C(bold red)%d%Creset %C(blue)%cr%Creset %C(dim green)%cn%Creset %s'
    wlog = log --graph --pretty='format:%C(yellow)%h%Creset%C(bold red)%d%Creset %C(blue)%cr%Creset %C(dim green)%cn%Creset %w(80,0,8)%s'

